{
  "name": "paletter",
  "version": "0.19.0",
  "description": "simple JS class to manage color palettes",
  "main": "index.umd.js",
  "es2015": "index.js",
  "type": "module",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.umd.js",
    "index.d.ts",
    "generate.mjs",
    "colors.json",
    "palettes.json",
    "lib/toCSS.mjs",
    "lib/toCSS.d.ts",
    "lib/toSVGviz.mjs",
    "lib/toSCSSvars.mjs",
    "lib/toHTML.mjs",
    "lib/tableView.mjs",
    "lib/color-helper-fn.scss",
    "lib/htmlExportCSS.css",
    "lib/parse-palettes.scss"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.umd.js",
      "types": "./index.d.ts"
    },
    "./lib/*.mjs": "./lib/*.mjs",
    "./lib/toCSS": {
        "import": "./lib/toCSS.mjs",
        "require": "./lib/toCSS.mjs",
        "types": "./lib/toCSS.d.ts"
    },
    "./lib/*.scss": "./lib/*.scss",
    "./lib/*.css": "./lib/*.css",
    "./colors.json": "./colors.json",
    "./palettes.json": "./palettes.json"
  },
  "scripts": {
    "test": "npm run test:jest && npm run test:cli",
    "test:jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:cli": "./generate.mjs -c ./test/colors.json -p ./test/palettes.json",
    "build": "rollup --config"
  },
  "bin": {
    "paletterTo": "./generate.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meodai/paletter.git"
  },
  "keywords": [
    "palette",
    "colors",
    "colours",
    "design-system",
    "design"
  ],
  "author": "meodai@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meodai/paletter/issues"
  },
  "homepage": "https://github.com/meodai/paletter#readme",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "babel-jest": "^29.7.0",
    "eslint": "^8.51.0",
    "eslint-config-google": "^0.9.1",
    "jest": "29.7",
    "rollup": "^4.1.4",
    "rollup-plugin-babel": "^3.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest",
      "^.+\\.mjs$": "babel-jest"
    },
    "collectCoverage": true,
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ]
  },
  "dependencies": {
    "culori": "^3.2.0"
  }
}
